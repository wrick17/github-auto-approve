{
  "manifest_version": 3,
  "name": "Github Auto Approve",
  "description": "Auto Approve PR on click of a button",
  "version": "1.2",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ],
      "matches": [
        "https://github.com/*/pull/*"
      ]
    }
  ]
}